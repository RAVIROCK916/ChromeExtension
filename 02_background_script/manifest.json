{
    "manifest_version": 2,
    "name" : "Chrome Extension 2",
    "version" : "1.0",
    "content_scripts" : [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : [
                "content.js"
            ]
        }
    ],
    "background" : {
        "scripts" : [
            "background.js"
        ],
        "persistent" : false
    },
    "browser_action" : {
        "default_icon" : "hacker.png"
    }
}
