{
    "manifest_version": 2,
    "name" : "Chrome Extension 4",
    "version" : "1.0",
    "content_scripts" : [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : [
                "content.js"
            ]
        }
    ],
    "browser_action" : {
        "default_icon" : "hacker.png",
        "default_popup" : "pop_up.html",
        "default_title" : "Hey there!"
    }
}
